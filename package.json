{
  "name": "prm-deductions-gp-to-repo",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "lint": "eslint src/ test/ scripts/",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint:fix && npm run test:unit ",
    "test:unit": "jest \"src/(?!(.*\\integration.test\\.js))\" --setupFiles dotenv/config ",
    "test:integration": "jest \"src/.*\\integration.test\\.js\" --detectOpenHandles --forceExit --setupFiles dotenv/config",
    "test:functional": "jest test/functional/*",
    "test:smoke": "jest test/smoke/*",
    "test:health": "jest test/functional/health.test.js",
    "test:docker": "npm run test:health",
    "test:watch": "jest \"src/(?!(.*\\integration.test\\.js))\" --setupFiles dotenv/config --watch",
    "test:coverage": "jest src/ --maxWorkers=1 --coverage --detectOpenHandles --forceExit --setupFiles dotenv/config",
    "build": "babel src -d build --ignore '**/*.test.js' && cp src/*.json build",
    "start": "node build/server.js",
    "start:local": "babel-node -r dotenv/config src/server.js",
    "start:nodemon": "nodemon --exec npm run start:local",
    "db:wait": "babel-node scripts/wait-for-postgres.js",
    "db:migrate": "npm run db:wait && sequelize-cli db:migrate",
    "db:teardown": "sequelize-cli db:migrate:undo:all",
    "db:setup": "sequelize-cli db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhsconnect/prm-deductions-gp-to-repo.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "jest": "^26.6.3",
    "jest-when": "^3.0.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "async-local-storage": "^2.3.1",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.7.0",
    "express-winston": "^4.0.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "swagger-ui-express": "^4.1.5",
    "traverse": "^0.6.6",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "jest": {
    "resetModules": true,
    "clearMocks": true,
    "automock": false
  }
}
